<div class="holder">
  <div class="robo-background">
  </div>
  <div class="card-holder">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-6">
          <%= simple_form_for(@robot) do |f| %>
          <%= f.input :name %>
          <%= f.input :photos, as: :file, input_html: { multiple: true } %>
          <%= f.submit "create a robot", class: 'btn btn-primary' %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
  <form method='POST' action="/robots"></form>
</div>
